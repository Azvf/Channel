{
  "name": "edge-extension",
  "version": "1.0.0",
  "description": "A modern Edge browser extension",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build && npm run obfuscate",
    "obfuscate": "node obfuscate.js",
    "dev": "vite build --watch",
    "preview": "vite --open /src/preview/dev-preview.html",
    "preview:build": "npm run build && vite preview --open /dev-preview.html",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ct": "playwright test --config=playwright-ct.config.ts",
    "test:ct:ui": "playwright test --config=playwright-ct.config.ts --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@chromatic-com/storybook": "^4.1.2",
    "@jest/globals": "^29.7.0",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-onboarding": "^10.0.6",
    "@storybook/addon-vitest": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.9.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^29.7.0",
    "chromatic": "^13.3.3",
    "eslint": "^8.45.0",
    "eslint-plugin-storybook": "^10.0.6",
    "identity-obj-proxy": "^3.0.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.35",
    "storybook": "^10.0.6",
    "tailwindcss": "^3.4.1",
    "terser": "^5.31.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.1.6",
    "vite": "^7.2.2",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "browser-extension",
    "edge",
    "chrome",
    "manifest-v3"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "lucide-react": "^0.552.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
